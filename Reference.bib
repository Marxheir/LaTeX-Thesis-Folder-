}

@article{akinola_foreign_2022,
	title = {Foreign Aid, Economic Growth And Health Outcomes In Nigeria},
	volume = {3},
	rights = {Copyright (c) 2022 {FUOYE} {JOURNAL} {OF} {FINANCE} {AND} {CONTEMPORARY} {ISSUES}},
	issn = {2814-1733},
	url = {https://www.fjfci.fuoye.edu.ng/index.php/fjfci/article/view/59},
	abstract = {Inflows of foreign aid to developing countries have increased while health outcomes are significantly low compared to other countries of the world. Therefore, there is a debate whether foreign aid contributes to improvement in health outcome in the literature. This research examines the impact of foreign aids, economic growth on health outcome in Nigeria using data from 1981 to 2019. Using the Auto Regressive Distributive Lag ({ARDL}) and Toda-Yamamoto techniques, the study examines the impact of government expenditure on health, economic growth and other development assistance on life expectancy. Data from World Development Indicators and Central Bank of Nigeria Statistical Bulletin were used. Official Development Assistance ({ODA}) showed a positive but not significant impact on life expectancy rate in Nigeria while government expenditure on health showed a positive and significant relationship, economic growth showed a non-significant relationship. The result from Toda-Yamamoto technique shows that there is a unidirectional causality running from foreign aids to life expectancy. The causal feedback relationship between life expectancy and government expenditure on health implies that increase in government spending on health facilities can lead to an improvement in life expectancy in Nigeria. Also, an improvement in health quality or life expectancy may increase the population which will in turn force the government to increase health spending so as to care for the citizens, especially the aging population. No evidence of directional causality between life expectancy and economic growth. Meanwhile, there is a one-way causality flowing from life expectancy to exchange rate in Nigeria. The research concluded that foreign aid is not effective in improving health outcome in Nigeria. The research recommended that government should increase her spending on health to improve the overall health outcome in Nigeria.},
	number = {2},
	journaltitle = {{FUOYE} {JOURNAL} {OF} {FINANCE} {AND} {CONTEMPORARY} {ISSUES}},
	author = {Akinola, Gbenga Wilfred and Asaolu, Adepoju Adeoba},
	urldate = {2023-11-02},
