	doi = {10.13140/RG.2.2.30427.36641},
	file = {Full Text PDF:C\:\\Users\\OWNER\\Zotero\\storage\\UVSPAJXT\\Chikwede - 2016 - AN EMPIRICAL INVESTIGATION OF FOREIGN AID EFFECTIV.pdf:application/pdf},
}

